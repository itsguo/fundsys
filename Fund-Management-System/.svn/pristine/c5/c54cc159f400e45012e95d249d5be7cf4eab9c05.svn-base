 public enum TokenState {  
	 /**
	  * è¿‡æœŸ
	  */
	EXPIRED("EXPIRED"),
	/**
	 * æ— æ•ˆ(tokenä¸åˆæ³?
	 */
	INVALID("INVALID"), 
	/**
	 * æœ‰æ•ˆçš?	 */
	VALID("VALID");  
	
    private String  state;  
      
    private TokenState(String state) {  
        this.state = state;  
    }
    
    /**
     * æ ¹æ®çŠ¶æ?å­—ç¬¦ä¸²è·å–tokençŠ¶æ?æšä¸¾å¯¹è±¡
     * @param tokenState
     * @return
     */
    public static TokenState getTokenState(String tokenState){
    	TokenState[] states=TokenState.values();
    	TokenState ts=null;
    	for (TokenState state : states) {
			if(state.toString().equals(tokenState)){
				ts=state;
				break;
			}
		}
    	return ts;
    }
    public String toString() {
    	return this.state;
    }
	public String getState() {
		return state;
	}
	public void setState(String state) {
		this.state = state;
	}
    
}  
